{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-router-dom": "^5.2.0",
    "@blueprintjs/core": "^3.36.0",
    "@apollo/client": "^3.2.9",
    "graphql": "^15.4.0",
    "crypto-js": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.53",
    "@types/react-helmet": "^6.1.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/crypto-js": "^4.0.1",
    "typescript": "^4.0.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack serve --watch",
    "build": "NODE_ENV=production webpack --progress"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.tsx",
          "**/*.ts"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
